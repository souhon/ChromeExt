{
    "manifest_version": 3,
    "name": "Audio Selector",
    "version": "1.0",
    "description": "",
    "action": {
       "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["content.js"]
        }
    ]

}